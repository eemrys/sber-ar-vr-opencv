<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_main"
    app:startDestination="@id/fragmentCamera">

    <fragment
        android:id="@+id/fragmentCamera"
        android:name="com.example.testapp.CameraFragment"
        tools:layout="@layout/fragment_camera" />

    <fragment
        android:id="@+id/fragmentResults"
        android:name="com.example.testapp.ResultsFragment"
        tools:layout="@layout/fragment_results">
        <argument
            android:name="results"
            app:argType="com.example.testapp.CameraInfoString"/>
    </fragment>

</navigation>